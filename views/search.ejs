<div class="author">
    <h1 class="author__heading">Search Results</h1>
</div>

<section class="articles">

    <ul class="articles__ul">
        <% data.forEach(post => { %>
         
        <li>
            <a href="/post/<%= post._id %>">
                <span><%= post.title %></span>
                <span class="articles-list__date"><%= post.createTime.toDateString() %></span>
            </a>
        </li>

        <% }) %>

    </ul>
</section>